<environmentSettings>
    <debug>true</debug>
    <environment>local</environment>
    <webSites>
        <MYWEBSITE>
            <physicalPathRoot>c:\inetpub</physicalPathRoot>
            <physicalFolderPrefix>MY-WEBSITE</physicalFolderPrefix>
            <bindings>
                <protocol>http</protocol>
                <port>80</port>
                <hostName>local.MY-WEBSITE.com</hostName>
            </bindings>
            <bindings>
                <protocol>https</protocol>
                <port>443</port>
                <hostName>local.MY-WEBSITE.com</hostName>
            </bindings>
            <siteName>local.MY-WEBSITE.com</siteName>
            <appPath>/</appPath>
            <!-- valid authenticationTypes are: [anonymousAuthentication, basicAuthentication, formsAuthentication, windowsAuthentication]-->
            <authenticationType>anonymousAuthentication</authenticationType>
            <appPool>
                <name>TransCanada.CLS.App</name>
                <!-- valid identityTypes are: [LocalSystem, LocalService, NetworkService, SpecificUser, ApplicationPoolIdentity] -->
                <identityType>NetworkService</identityType>
                <!-- Set this value to the User the Service will run under in the format DOMAIN\username -->
                <!-- If Running as 'NetworkService' then 'NT AUTHORITY\Network Service' is used -->
                <userName>NT AUTHORITY\Network Service</userName>
                <!-- Leave blank unless using SpecificUser -->
                <password></password>
                <maxWorkerProcesses>1</maxWorkerProcesses>
            </appPool>
        </MYWEBSITE>
    </webSites>

    <databases>
        <MYDATABASE>
            <connectionString>Server=myServerAddress;Database=myDataBase;Trusted_Connection=True;</connectionString>
        </MYDATABASE>
    </databases>
</environmentSettings>